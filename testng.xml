<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="CrossBrowser Test Suite" parallel=" ">
    <test name = "ChromeTest" verbose="10" preserve-order="true">
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="com.lumatest.test.NavigationTest"/>
        </classes>
    </test>

    <test name = "FirefoxTest" verbose="10" preserve-order="true">
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="com.lumatest.test.NavigationTest"/>
        </classes>
    </test>

    <test name = "EdgeTest" verbose="2" preserve-order="true">
        <parameter name="browser" value="edge" />
        <classes>
            <class name="com.lumatest.test.NavigationTest"/>
        </classes>
    </test>

</suite>